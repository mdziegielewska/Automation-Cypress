    "@tsconfig/cypress": "^1.0.2",
    "@types/mocha": "^10.0.7",
    "@types/mochawesome": "^6.2.4",
    "cross-spawn": "^7.0.6",
    "cypress": "^13.13.2",
    "cypress-file-upload": "^5.0.8",
    "cypress-mochawesome-reporter": "^3.8.2",
    "cypress-real-events": "^1.13.0",
    "cypress-upload-file-post-form": "^1.1.0",
    "typescript": "^5.5.4"
